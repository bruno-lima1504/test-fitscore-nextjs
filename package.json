{
  "name": "legal-fitscore",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run services:up && npm run services:wait:database && npm run prisma:generate && npm run migrations:up && npm run db:seed && next dev",
    "vercel-build": "npm run prisma:generate && npm run migrations:up && npm run prisma:seed && next build",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "services:up": "docker compose -f infra/compose.yml up -d",
    "services:down": "docker compose -f infra/compose.yml down",
    "services:stop": "docker compose -f infra/compose.yml stop",
    "services:wait:database": "node infra/scripts/wait-for-postgres.js",
    "test:watch": "jest --watchAll --runInBand --verbose",
    "test": "concurrently -n next,jest --hide next -k -s command-jest \"next dev\" \"jest --runInBand --verbose\"",
    "db:seed": "prisma db seed",
    "lint:prettier:check": "prettier --check .",
    "lint:prettier:fix": "prettier --write .",
    "lint:eslint:check": "next lint",
    "prisma:generate": "npx prisma generate",
    "prisma:seed": "npx prisma db seed",
    "migrations:up": "npx prisma migrate deploy"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.4.0",
    "@hookform/resolvers": "^3.3.4",
    "@prisma/client": "^5.19.1",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-radio-group": "^1.2.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slider": "^1.2.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@react-email/components": "^0.5.1",
    "@tanstack/react-table": "^8.20.5",
    "@types/jest": "^30.0.0",
    "async-retry": "^1.3.3",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "concurrently": "^8.2.2",
    "cookies-next": "^6.1.0",
    "jose": "^6.0.13",
    "lucide-react": "^0.446.0",
    "next": "15.0.3",
    "next-themes": "^0.3.0",
    "node-cron": "^3.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "resend": "^4.0.0",
    "swr": "^2.3.6",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.19.1",
    "zod": "^3.23.8"
  },
  "prisma": {
    "seed": "tsx ./prisma/seed.ts"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/async-retry": "^1.4.9",
    "@types/node": "^22.7.4",
    "@types/node-cron": "^3.0.11",
    "@types/node-fetch": "^2.6.13",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.0.3",
